<div id="gs-search-file-site-home">
  <div class="gs-search search" id="gs-search-file-site-home-search">
    <div class="gs-search-entry" style="display:none;">
      <input type="text" class="textEntry"/>
      <button>Search</button>
    </div>
    <p id="gs-search-file-site-home-loading"
       class="gs-search-loading" style="display:none;">
      <span data-icon="&#xe619;" aria-hidden="true" class="loading"> </span>
      Loading the files&#8230;
    </p><!--gs-search-file-site-home-loading-->
    <div id="gs-search-file-site-home-latest"
         class="gs-search-results">
      &#160;
    </div><!--gs-search-file-site-home-latest-->
    <div id="gs-search-file-site-home-toolbar"
         class="toolbar gs-search-toolbar">
      <span class="ui-widget-header ui-corner-all">
        <button id="gs-search-file-site-home-toolbar-prev"
                class="gs-search-toolbar-previous">Newer</button>
        <button id="gs-search-file-site-home-toolbar-next"
                class="gs-search-toolbar-next">Older</button>
      </span>
    </div><!--gs-search-file-site-home-toolbar-->
  </div><!--gs-search-->
  <div id="gs-search-file-site-home-tasks">
    <ul class="inline-list">
      <li><a type="application/atom+xml" class="alternateATOM"
             tal:define="title string:Posts in ${view/siteInfo/name};"
             href="/s/search.atom?t=0&amp;f=1&amp;p=0">Web feed</a></li>
    </ul>
  </div><!--gs-search-file-site-home-tasks-->
</div><!--gs-search-file-site-home-->
